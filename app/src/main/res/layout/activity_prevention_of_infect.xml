<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".subActivity.preventionOfInfect.PreventionOfInfectActivity">
    <androidx.appcompat.widget.Toolbar
        android:layout_height="?attr/actionBarSize"
        android:layout_width="match_parent"
        android:background="@color/colorPrimary"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        android:id="@+id/toolbar_preventionOfInfect"
        app:layout_constraintTop_toTopOf="parent"
        app:title="감염 예방 수칙"
        ></androidx.appcompat.widget.Toolbar>
    <WebView
        android:layout_margin="4dp"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/toolbar_preventionOfInfect"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/webView_preventionOfInfect"/>
</androidx.constraintlayout.widget.ConstraintLayout>